{
  "name": "wtelno-info-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./src/* -c ./.eslintrc.json",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write -l",
    "stylelint": "stylelint \"**/**.scss\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "fix": "npm run lint:fix && npm run format:fix && npm run stylelint:fix",
    "check-types": "tsc --noEmit",
    "commit": "cz",
    "prebuild": "npm run check-types && npm run format && npm run lint && npm run stylelint"
  },
  "dependencies": {
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "next": "13.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "stylelint": "^15.6.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^8.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
